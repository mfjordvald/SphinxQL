{
    "name": "mnshankar/sphinxql",
    "description": "Integrate SphinxQL with Laravel 6. Simplifies Laravel-Sphinx(RT) search",
    "license":"MIT",
    "keywords": ["Laravel","Laravel6","SphinxQL", "Sphinx RT", "Search"],
    "authors": [
        {
            "name": "Shankar Manamalkav",
            "email": "mnshankar@yahoo.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "8.*",
        "foolz/sphinxql-query-builder": "dev-php8"
    },
    "autoload": {
        "psr-0": {
            "mnshankar\\Sphinxql": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "mnshankar\\Sphinxql\\SphinxqlServiceProvider"
            ],
            "aliases": {
                "SphinxQL": "mnshankar\\Sphinxql\\Facades\\SphinxqlFacade"
            }
        }
    }
}
