{
    "name": "mfjordvald/sphinxql",
    "description": "Integrate SphinxQL with Laravel 8. Simplifies Laravel-Sphinx(RT) search",
    "license":"MIT",
    "keywords": ["Laravel","Laravel8","SphinxQL", "Sphinx RT", "Search"],
    "authors": [
        {
            "name": "Martin Fjordvald",
            "email": "martin@evilgeniusmedia.org"
        },
        {
            "name": "Shankar Manamalkav",
            "email": "mnshankar@yahoo.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": ">9.0",
        "foolz/sphinxql-query-builder": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "mfjordvald\\Sphinxql": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "mfjordvald\\Sphinxql\\SphinxqlServiceProvider"
            ],
            "aliases": {
                "SphinxQL": "mfjordvald\\Sphinxql\\Facades\\SphinxqlFacade"
            }
        }
    }
}
